{
  "name": "demo03-ssr-vite-vue-02",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node server",
    "serve": "NODE_ENV=production node --experimental-json-modules server",
    "build": "npm run build:client && npm run build:server",
