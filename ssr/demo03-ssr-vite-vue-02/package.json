  },
  "dependencies": {
    "express": "^4.18.2",
    "pinia": "^2.0.23",
    "vue": "^3.2.37",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.1.0",
    "compression": "^1.7.4",
    "vite": "^3.1.0"
  }
}
